{
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"module": "commonjs",
		"strict": true,
		"noImplicitAny": true,
		"noLib": true,
		"downlevelIteration": true,
		"declaration": true,
		"target": "es6",
		"typeRoots": ["node_modules/@rbxts"],
		"allowSyntheticDefaultImports": true
	},
	"typeAcquisition": {
		"enable": true
	}
}